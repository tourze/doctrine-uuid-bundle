{
  "name": "tourze/doctrine-uuid-bundle",
  "description": "Doctrine UUID",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.20 || ^3.0",
    "symfony/framework-bundle": "^6.4",
    "symfony/property-access": "^6.4",
    "symfony/uid": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/bundle-dependency": "0.0.*",
    "tourze/doctrine-entity-checker-bundle": "0.0.*",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineUuidBundle\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
